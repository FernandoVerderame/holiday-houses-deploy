{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "build": {
        "env": {
            "DATABASE_URL": "mysql://root:root@localhost:3306/HH-deploy"
        },
        "commands": [
            "npm run build"
        ]
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}